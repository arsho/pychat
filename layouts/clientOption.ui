<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="tk.Toplevel" id="connection_options">
    <property name="height">200</property>
    <property name="takefocus">false</property>
    <property name="title" translatable="yes">Connection options</property>
    <property name="width">200</property>
    <child>
      <object class="tk.Label" id="label2">
        <property name="text" translatable="yes">Server IP:</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Entry" id="loc">
        <property name="justify">center</property>
        <property name="text" translatable="yes">127.0.0.1</property>
        <property name="validate">focus</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Label" id="label3">
        <property name="anchor">nw</property>
        <property name="justify">center</property>
        <property name="takefocus">true</property>
        <property name="text" translatable="yes">Port:</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">w</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Entry" id="port">
        <property name="justify">center</property>
        <property name="text" translatable="yes">9999</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Button" id="button1">
        <property name="command" type="command" cbtype="simple">on_connect</property>
        <property name="compound">bottom</property>
        <property name="justify">right</property>
        <property name="text" translatable="yes">connect</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
